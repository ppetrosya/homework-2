function number(num) {
    if (num.length === 1) {
       return num;
    }
    let lastDigit = num[num.length - 1];
    let firstDigit = num[0];
    let midl = "";
    for (i = 0; i < num.length; i++){
        if ( i !== 0 && i !== num.length - 1){
            midl += num[i];
        }
    }
    return lastDigit + midl + firstDigit;
}
console.log(number("2"))
